                        }
                    }
                },
                "security": [
                    {
                        "Bearer Token": [
                            "write:Token",
                            "read:Token"
                        ]
                    }
                ]
            }
        },
        "v1/Login": {
            "get": {
                "tags": [
                    "Login"
